<header>
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <h1><%= link_to "Catpus", "/#" %></h1>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <% if logged_in? %>
              <li class="nav-li"><span id='username'><%= current_user.name %> </span></li>
              <li class="nav-li"> <%= link_to "Dashboard", ('/#dashboard') %> </li>
              <li class="nav-li"> <%= button_to "Sign Out", sessions_url, method: :delete %> </li>
            <% else %>
              <%= link_to "Sign in with Github", "/auth/github/" %>
            <% end %>
          </ul>
        </div>
      </div>
    </nav>
  </header>
