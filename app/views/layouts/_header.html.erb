<header>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="/#"><h1 id='catpus-icon'><%= image_tag "tentacle.png", class: "head-ico" %>  Tentacle</h1></a>
        </div>

          <ul class="nav navbar-nav navbar-right">
            <% if logged_in? %>
              <li class="nav-li" id="user-name-img"><img class='thumbnail-header' src="<%= current_user.avatar_url %>"><span id='username'><%= current_user.name %></span></li>
              <li class="nav-li"> <%= link_to "Dashboard", ('/#dashboard') %> </li>
              <li class="nav-li"> <%= button_to "Sign Out", sessions_url, method: :delete %> </li>
            <% else %>
              <a class="btn btn-social-icon btn-github" href="/auth/github/">
                <i class="fa fa-github"></i>
              </a>
            <% end %>
          </ul>
      </div>
    </nav>
  </header>
