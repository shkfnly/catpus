<h4>Create a Repo Tracker</h4>
<input type='hidden' name="repo[user_id]" value='<%= this.user.id %>'
<label for='title'>Title:</label>
<input type='text' id='title' name= "repo[title]" value='<%= model.title %>' />
<select>
  <% repositories.each(function(repository){ %>
    <option value = "<%= repository.get('url')  + '=' + repository.id %>"> <%= repository.get('name') %> </option>
  <% }) %>
</select>
<input type='submit' class='submit' value='Submit'>

